<Application x:Class="EasyScan.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EasyScan"
             xmlns:controls="clr-namespace:EasyScan.Controls"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <controls:MediaInformationConverter x:Key="MediaInformationConverter" />
            <vm:ViewModelLocator x:Key="Locator" xmlns:vm="clr-namespace:EasyScan.ViewModel" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
